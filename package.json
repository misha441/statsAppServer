    "statistics",
    "users"
  ],
  "author": "Mykhailo Debelyak <mihadebelyak@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "config": "^3.3.6",
    "express": "^4.17.2",
    "mongoose": "^7.0.0",
    "sqlite3": "^5.0.2"
  },
  "devDependencies": {
    "concurrently": "^6.5.1",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.15"
  }
}
