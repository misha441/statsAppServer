{
  "name": "userstatistics",
  "version": "1.0.0",
  "description": "site with users and their statistics",
  "main": "app.js",
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node app.js",
    "server": "nodemon app.js",
    "client": "npm run startDev --prefix client",
